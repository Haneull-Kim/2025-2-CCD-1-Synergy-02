{
  "expo": {
    "name": "BokjaApp",
    "slug": "bokja-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icons/main_icon/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/icons/main_icon/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#FFCC02"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icons/main_icon/icon.png",
        "backgroundColor": "#FFCC02"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "favicon": "./assets/icons/main_icon/icon.png"
    }
  }
}
